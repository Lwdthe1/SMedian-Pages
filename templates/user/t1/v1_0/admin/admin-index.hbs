            <input ng-model="page.draftData.featuredStoriesSection.title" ng-change="onDraftDataChange()"  class="form-field w-input" id="user-hero-title" maxlength="256" name="user-hero-title" placeholder="Featured Stories"  type="text">
            <label class="input-field-label">Subtitle</label>
            <input ng-model="page.draftData.featuredStoriesSection.subtitle" ng-change="onDraftDataChange()" class="form-field w-input"  id="user-hero-subtitle" maxlength="256" name="user-hero-subtitle" placeholder="These are my featured stories"  type="text">
            <div class="form-fields-row w-row">
              <div class="column-3 w-col w-col-3" style="margin-right:5px" 
                ng-repeat="featuredStory in page.draftData.featuredStoriesSection.stories | orderBy:'position'">
                <label class="input-field-label" ><b>#{{featuredStory.position + 1}}</b> Story</label>
                <input ng-model="featuredStory.url" ng-change="onDraftDataChange(); getFeaturedMediumArticleData(featuredStory)" class="form-field w-input" id="user-featured-story-url-12" maxlength="256" name="user-featured-story-url-12" placeholder="https://medium.com/..."  type="text">
                <input ng-model="featuredStory.title" ng-change="onDraftDataChange()" class="form-field w-input" id="user-featured-story-url-12" maxlength="256" name="user-featured-story-url-12" placeholder="Title"  type="text">
                <input ng-model="featuredStory.description" ng-change="onDraftDataChange()" class="form-field w-input" id="user-featured-story-url-12" maxlength="256" name="user-featured-story-url-12" placeholder="Description"  type="text">
                <input ng-model="featuredStory.imageUrl" ng-change="onDraftDataChange()" class="form-field w-input" id="user-featured-story-url-12" maxlength="256" name="user-featured-story-url-12" placeholder="http://example.com/images/example.png"  type="text">
                <img ng-src="featuredStory.imageUrl" ng-show="featuredStory.imageUrl.length">
                <button class="clean-button clean-button--plain" ng-click="openImageGallery('story', featuredStory)"><img src="/img/icon_gallery.svg" class="smpscss_u-size16x16 smpscss_u-marginRight10"/></button>
                <i class="clean-text text-s ct-l-green" style="margin-top:5px; margin-bottom:10px" ng-show="fetchingFeatureStoryUrls[featuredStory.url.trim()]">
                  Loading title, description, and image url ...
                </i>
                <button class="clean-button clean-button--plain ct-l-red" ng-click="removeFeaturedStory(featuredStory)">
                  Remove
                </button>
              </div>
            </div>
            <button class="clean-button clean-button--small" style="margin-top:10px"
              ng-show="page.draftData.featuredStoriesSection.stories.length < 100"
              ng-click="addContainerFor('featuredStories')">
              Add Another Story
            </button>
          </div>
        </div>
      </div>
      <div class="admin-section">
        <h2 class="heading heading-admin-section">Footer Section</h2>
        <div class="form-wrapper">
          <form class="form" id="email-form" name="email-form">
            <label class="input-field-label">Title</label>
            <input ng-model="page.draftData.footerSection.title" ng-change="onDraftDataChange()" class="form-field w-input"id="user-hero-background-image-2" maxlength="256" name="user-hero-background-image-2" placeholder="Your Name"  type="text">
