            <label class="input-field-label" >Success Link Text</label>
            <input ng-model="page.draftData.contactForm.successCTAText" ng-change="onDraftDataChange()" class="form-field w-input" id="user-theme-color-4" maxlength="256" name="user-theme-color-4" placeholder="Get Our Free E-Book"  type="text">
          </form>
        </div>
      </div>
      <div class="admin-section">
        <h3 class="heading heading-admin-section">Social Links</h3>
        <div class="form-wrapper w-form">
          <form class="form" id="email-form" name="email-form">
            <div class="form-fields-row w-row">
              <div class="column-3 w-col w-col-3" style="margin-right:5px; margin-bottom:10px"
                ng-repeat="socialLink in page.draftData.socialLinks">
                <label class="input-field-label" >{{socialLink.name}}</label>
                <input ng-model="socialLink.url" ng-change="onDraftDataChange()" class="form-field w-input" placeholder="{{socialLink.inputPlaceholder}}" type="text">
                <div class="checkbox-field w-checkbox">
                  <input ng-model="socialLink.showInHeroSection" ng-change="onDraftDataChange()" class="w-checkbox-input" id="checkbox-2" name="checkbox-2" type="checkbox">
                  <label class="form-field-checkbox-label w-form-label" >Show in Hero Section</label>
                </div>
                <div class="checkbox-field w-checkbox">
                  <input ng-model="socialLink.showInAboutSection" ng-change="onDraftDataChange()" class="w-checkbox-input" id="checkbox-2" name="checkbox-2" type="checkbox">
                  <label class="form-field-checkbox-label w-form-label" >Show in About Section</label>
                </div>
                <div class="checkbox-field w-checkbox">
                  <input ng-model="socialLink.showInFooterSection" ng-change="onDraftDataChange()" class="w-checkbox-input" id="checkbox-2" name="checkbox-2" type="checkbox">
                  <label class="form-field-checkbox-label w-form-label" >Show in Footer Section</label>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
      <div class="admin-section">
        <h3 class="heading heading-admin-section">Featured Stories</h3>
        <div class="form-wrapper w-form">
          <div class="form">
            <div class="checkbox-field w-checkbox">
              <input ng-model="page.draftData.featuredStoriesSection.show" ng-change="onDraftDataChange()" class="w-checkbox-input"  id="checkbox-2" name="checkbox-2" type="checkbox">
              <label class="form-field-checkbox-label w-form-label" >Show Section</label>
            </div>
