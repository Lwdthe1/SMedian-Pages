              <button ng-click="openImageGallery('page.draftData.footerSection.backgroundImageUrl')"><img src="/img/icon_gallery.svg" class="smpscss_u-size16x16 smpscss_u-marginRight10"/></button>
            </label>
            <input ng-model="page.draftData.footerSection.backgroundImageUrl" ng-change="onDraftDataChange()" class="form-field w-input"id="user-hero-background-image-2" maxlength="256" name="user-hero-background-image-2" placeholder="https://yourwebsite.com/images/background.png"  type="text">
            <label class="input-field-label">Background Color</label>
            <input ng-model="page.draftData.footerSection.backgroundColor" ng-change="onDraftDataChange()" class="form-field w-input jscolor" value="#1abc9c"  id="user-hero-text-color" maxlength="256" name="user-hero-text-color" placeholder="#1abc9c"  type="text">
            <label class="input-field-label">Text Color</label>
            <input ng-model="page.draftData.footerSection.textColor" ng-change="onDraftDataChange()" class="form-field w-input jscolor" value="ffffff" id="user-theme-color-4" maxlength="256" name="user-theme-color-4" placeholder="#ffffff"  type="text">
            <h3 class="heading heading-admin-subsection">Links</h3>
            <div class="form-fields-row w-row">
              <div class="column-4 w-col w-col-4" style="margin-right:5px"
                ng-repeat="footerLink in page.draftData.footerSection.links">
                <label class="input-field-label" >Link Display Text</label>
                <input ng-model="footerLink.text" ng-change="onDraftDataChange()" class="form-field w-input"id="user-footer-link--display-text" maxlength="256" name="user-footer-link--display-text" placeholder="User Name" type="text">
                <label class="input-field-label" >Link URL</label>
                <input ng-model="footerLink.url" ng-change="onDraftDataChange()" class="form-field w-input" id="user-footer-link-4" maxlength="256" name="user-footer-link-4" placeholder="https://www.medium.com/@username" type="text">
                <button class="clean-button clean-button--plain ct-l-red" ng-click="removeFooterLink(footerLink)">
                  Remove
                </button>
              </div>
            </div>
            <button class="clean-button clean-button--small" style="margin-top:10px" ng-show="page.draftData.footerSection.links.length < 3" ng-click="addContainerFor('footerLinks')">
              Add Another
            </button>
          </form>
        </div>
      </div>
    </div>
  </div>


